var list = ["a string","another string"]
print("Initial list:")
print(list)

var list = list + "yet another string"
print("\nAppend:")
print(list)

var list = list - 0
print("\nRemove 0th:")
print(list)

var list = list <= ["more strings", "more and more"]
print("\nConcat:")
print(list)

print("\nGet 0th:")
print(list > 0)