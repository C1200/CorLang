func happybirthday(name, age)
    var output = "Happy Birthday To You\n" * 2
    var output = output + "Happy Birthday To " + name
    var output = output + "\nHappy Birthday To You\n"
    var output = output + "Hip, Hip! Hooray!\n" * age
    return output
end

var name = input("Name: ")
var age = input_int("Age: ")

print(happybirthday(name, age))